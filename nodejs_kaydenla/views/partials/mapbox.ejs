<!DOCTYPE html>
<html>
<head>
<link rel="stylesheet" href="https://unpkg.com/leaflet@1.7.1/dist/leaflet.css" integrity="sha512-xodZBNTC5n17Xt2atTPuE1HxjVMSvLVW9ocqUKLsCC5CXdbqCmblAshOMAS6/keqq/sMZMZ19scR4PsZChSR7A==" crossorigin="" />
<script src="https://unpkg.com/leaflet@1.7.1/dist/leaflet.js" integrity="sha512-XQoYMqMTK8LvdxXYG3nZ448hOEQiglfqkJs1NOQV44cWnUrBc8PkAOcXy20w0vlaXaVUearIOBhiXZ5V3ynxwA==" crossorigin=""></script>
<style>
#map { height: 400px; }
</style>
</head>
<body>
     <div id="map"></div>
     <script>
        var mymap = L.map('map').setView([29.734370, -95.410973], 18);
        L.tileLayer('https://api.mapbox.com/styles/v1/{id}/tiles/{z}/{x}/{y}?access_token={accessToken}', {
            attribution: 'Map data &copy; <a href="https://www.openstreetmap.org/copyright">OpenStreetMap</a> contributors, Imagery Â© <a href="https://www.mapbox.com/">Mapbox</a>',
            maxZoom: 18,
            id: 'mapbox/streets-v11',
            tileSize: 512,
            zoomOffset: -1,
            accessToken: 'sk.eyJ1IjoibW9oYW5yZWRkeXB1dGx1cnUiLCJhIjoiY2t2dTYybDZ2M2xuZDJ3cWdrZWwzYzljMSJ9.bVFYAXauU7-pc28IIZTzOQ'
        }).addTo(mymap);
		
		var myIcon = L.icon({
			iconUrl: 'https://external-content.duckduckgo.com/iu/?u=https%3A%2F%2Fapi.time.com%2Fwp-content%2Fuploads%2F2016%2F01%2Fkfc-nashville-hot-chicken.jpg&f=1&nofb=1&ipt=0321b242bd8e74b1f464570a0b06390ace91fe3ef9baa2c9c580dead261a206d&ipo=images',
			iconSize: [38, 95],
			iconAnchor: [22, 94],
			popupAnchor: [-3, -76],
			shadowUrl: 'https://external-content.duckduckgo.com/iu/?u=https%3A%2F%2Fapi.time.com%2Fwp-content%2Fuploads%2F2016%2F01%2Fkfc-nashville-hot-chicken.jpg&f=1&nofb=1&ipt=0321b242bd8e74b1f464570a0b06390ace91fe3ef9baa2c9c580dead261a206d&ipo=images',
			shadowSize: [68, 95],
			shadowAnchor: [22, 94]
		});

        var marker = L.marker([29.734370, -95.410973]).addTo(mymap);

		var polygon = L.polygon([
            [29.734670, -95.411473],
            [29.734170, -95.410673],
            [29.734370, -95.410373]
        ]).addTo(mymap);
        polygon.bindPopup("I am a kaydens polygon.");

		var circle = L.circle([29.73370, -95.410973], {
			color: 'red',
			fillColor: '#f03',
			fillOpacity: 0.5,
			radius: 50
		}).addTo(mymap);

		var marker2 = L.marker([29.73370, -95.410973], {icon: myIcon}).addTo(mymap);
		marker2.bindPopup("<b>kayden La</b><br>Where I ate Howdy Hot chicken.").openPopup();



        var popup = L.popup()
        .setLatLng([29.7345, -95.410973])
        .setContent("this is Howdy hot chicken! I like this place.")
        .openOn(mymap);

		var popup = L.popup()
		.setLatLng([29.7345, -95.412273])
		.setContent("I am a custom popup that likes to eat howdy hot chicken.<br><i>very far away</i>")
		.openOn(mymap);



        var greenIcon = L.icon({
        iconUrl: 'https://external-content.duckduckgo.com/iu/?u=http%3A%2F%2F4pack.files.wordpress.com%2F2010%2F01%2Fburger-king.jpg&f=1&nofb=1&ipt=1608e0a0a96704d388c6a6538a945d0ebacda68824e23548682fdb04b7ea3d54&ipo=images',

        iconSize:     [38, 95], // size of the icon
        shadowSize:   [50, 64], // size of the shadow
        iconAnchor:   [22, 94], // point of the icon which will correspond to marker's location
        shadowAnchor: [4, 62],  // the same for the shadow
        popupAnchor:  [-3, -76] // point from which the popup should open relative to the iconAnchor
        });
        L.marker([29.733870, -95.40973], {icon: greenIcon}).addTo(mymap);
     </script>
</body>
</html>

